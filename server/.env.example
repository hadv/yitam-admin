# Server configuration
PORT=3001

# Environment (development/production)
NODE_ENV=development

# URL for Qdrant vector database
# Use http://localhost:6333 for local development with docker-compose
# Use http://qdrant:6333 for internal docker communication
QDRANT_URL=http://localhost:6333

# Qdrant configuration
QDRANT_API_KEY=your_qdrant_api_key_here
COLLECTION_NAME=your_collection_name

# Google Gemini API key
GEMINI_API_KEY=your_api_key_here

# Vector size for embeddings
GEMINI_VECTOR_SIZE=768

# Google Cloud Vision API configuration
GOOGLE_CREDENTIALS_BASE64=your_api_key_here

# Google Cloud Project ID
GOOGLE_CLOUD_PROJECT_ID=your_project_id

# Google OAuth2 client
GOOGLE_CLIENT_ID=your-client-id-here
GOOGLE_CLIENT_SECRET=your-client-secret-here
GOOGLE_REDIRECT_URI=http://localhost:3001/api/auth/google/callback
SESSION_SECRET=some-strong-random-string

# OCR Configuration
MAX_FILE_SIZE_MB=10 